{
  "cli": {
    "version": ">= 5.2.0",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "env": {
        "EXPO_NO_GIT_STATUS": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "autoIncrement": true
      },
      "env": {
        "EXPO_NO_GIT_STATUS": "1"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "env": {
        "EXPO_NO_GIT_STATUS": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
